<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="licence.meme.worrynot.fragments.MethodManagerFragment">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background720x1280_plain">

        <ImageView
            android:id="@+id/avatar_method_manager_fragment_iv"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="8dp"
            android:src="@drawable/avatar_circle"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/name_method_manager_fragment_et"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/et1"
            android:ems="10"
            android:text="USER"
            android:textAlignment="center"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/avatar_method_manager_fragment_iv"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/experience_bar_method_manager_fragment_iv"
            android:layout_width="150dp"
            android:layout_height="50dp"
            android:layout_marginLeft="8dp"

            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:src="@drawable/progress_bar_filled"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/avatar_method_manager_fragment_iv"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/name_method_manager_fragment_et" />

        <Button
            android:id="@+id/publish_method_manager_fragment_btn"
            android:layout_width="205dp"
            android:layout_height="40dp"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="8dp"
            android:background="@drawable/button_custom"
            android:text="PUBLISH"
            android:textAlignment="center"
            android:textColor="#21150f"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/menu_method_manager_fragment_btn"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/add_method_manager_fragment_btn"
            app:layout_constraintVertical_bias="0.428" />

        <Button
            android:id="@+id/add_method_manager_fragment_btn"
            android:layout_width="205dp"
            android:layout_height="40dp"
            android:layout_marginRight="8dp"
            android:background="@drawable/button_custom"
            android:text="ADD"
            android:textAlignment="center"
            android:textColor="#21150f"
            android:textStyle="bold"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/scrollView2"
            app:layout_constraintLeft_toRightOf="@+id/menu_method_manager_fragment_btn"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.0" />

        <Button
            android:id="@+id/menu_method_manager_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/menu_button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.007"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/avatar_method_manager_fragment_iv"
            app:layout_constraintVertical_bias="0.0" />

        <Button
            android:id="@+id/info_method_manager_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/info_button"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/menu_method_manager_fragment_btn" />

        <Button
            android:id="@+id/change_password_method_manager_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/change_password_button"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/info_method_manager_fragment_btn" />

        <Button
            android:id="@+id/logout_method_manager_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="10dp"
            android:background="@drawable/logout_button"
            app:layout_constraintBottom_toTopOf="@+id/add_method_manager_fragment_btn"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/change_password_method_manager_fragment_btn"
            app:layout_constraintVertical_bias="0.0" />


        <EditText
            android:id="@+id/editText"
            android:layout_width="205dp"
            android:layout_height="30dip"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/et1"
            android:text="Title"
            android:textColor="#AF5910"
            android:textColorHint="#AF5910"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/menu_method_manager_fragment_btn"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/experience_bar_method_manager_fragment_iv" />

        <EditText
            android:id="@+id/editText2"
            android:layout_width="205dp"
            android:layout_height="30dip"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/et1"
            android:text="Desciption"
            android:textColor="#AF5910"
            android:textColorHint="#AF5910"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/menu_method_manager_fragment_btn"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText" />

        <EditText
            android:id="@+id/editText3"
            android:layout_width="205dp"
            android:layout_height="30dip"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/et1"
            android:text="Story"
            android:textColor="#AF5910"
            android:textColorHint="#AF5910"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/menu_method_manager_fragment_btn"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText2" />

        <ScrollView
            android:id="@+id/scrollView2"
            android:layout_width="205dp"
            android:layout_height="180dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="16dp"
            android:background="@drawable/et1"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/menu_method_manager_fragment_btn"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText3"></ScrollView>
        <!--<LinearLayout-->
        <!--android:id="@+id/method_fragment_ll"-->
        <!--android:layout_width="205dp"-->
        <!--android:layout_height="300dp"-->
        <!--android:orientation="horizontal"-->
        <!--android:layout_marginRight="8dp"-->
        <!--app:layout_constraintRight_toRightOf="parent"-->
        <!--android:layout_marginLeft="8dp"-->
        <!--android:layout_marginBottom="8dp"-->
        <!--app:layout_constraintBottom_toTopOf="@+id/add_method_manager_fragment_btn"-->
        <!--app:layout_constraintHorizontal_bias="0.555"-->
        <!--android:layout_marginTop="8dp"-->
        <!--app:layout_constraintTop_toBottomOf="@+id/experience_bar_method_manager_fragment_iv"-->
        <!--app:layout_constraintLeft_toLeftOf="parent"-->
        <!--app:layout_constraintVertical_bias="0.111">-->

        <!--</LinearLayout>-->

    </android.support.constraint.ConstraintLayout>


</FrameLayout>
