<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="licence.meme.worrynot.fragments.HomeFragment">

    <android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background720x1280_plain">
        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/avatar_home_fragment_iv"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/avatar_circle"
            android:layout_marginLeft="10dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp" />

        <TextView
            android:id="@+id/name_home_fragment_tv"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:background="@drawable/et1"
            android:text="USER"
            android:textAlignment="center"
            android:ems="10"
            app:layout_constraintLeft_toRightOf="@+id/avatar_home_fragment_iv"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintHorizontal_bias="0.0" />
        <ProgressBar
            android:id="@+id/experience_bar_home_fragment_pb"
            android:layout_width="150dp"
            android:layout_height="10dp"
            style="?android:attr/progressBarStyleHorizontal"
            android:progressDrawable="@drawable/m_custom_progressbar"
            app:layout_constraintLeft_toRightOf="@+id/avatar_home_fragment_iv"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/name_home_fragment_tv"
            app:layout_constraintHorizontal_bias="0.0"
            android:layout_marginBottom="13dp"
            app:layout_constraintBottom_toTopOf="@+id/method_fragment_ll" />
        <Button
            android:id="@+id/change_home_fragment_btn"
            android:layout_width="205dp"
            android:layout_height="40dp"
            android:background="@drawable/button_custom"
            android:text="CHANGE"
            android:textAlignment="center"
            android:textColor="#21150f"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.94" />

        <Button
            android:id="@+id/submit_home_fragment_btn"
            android:layout_width="205dp"
            android:layout_height="40dp"
            android:background="@drawable/button_custom"
            android:text="Submit"
            android:textAlignment="center"
            android:textColor="#21150f"
            android:textStyle="bold"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.505"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.849" />

        <Button
            android:id="@+id/menu_home_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/menu_button"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.007"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/avatar_home_fragment_iv"
            app:layout_constraintVertical_bias="0.0" />
        <Button
            android:id="@+id/info_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/info_button"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/menu_home_fragment_btn"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.0" />
        <Button
            android:id="@+id/change_password_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/change_password_button"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/info_fragment_btn"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.0" />
        <Button
            android:id="@+id/logout_fragment_btn"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@drawable/logout_button"
            android:layout_marginTop="10dp"
            app:layout_constraintTop_toBottomOf="@+id/change_password_fragment_btn"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/submit_home_fragment_btn"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintVertical_bias="0.0" />
        <LinearLayout
            android:id="@+id/method_fragment_ll"
            android:layout_width="205dp"
            android:layout_height="300dp"
            android:orientation="horizontal"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginLeft="8dp"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/submit_home_fragment_btn"
            app:layout_constraintHorizontal_bias="0.555"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/experience_bar_home_fragment_pb"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintVertical_bias="0.111" />
    </android.support.constraint.ConstraintLayout>



</FrameLayout>
