<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
    <shape>
        <gradient
            android:startColor="#743D0A"
            android:centerColor="#743D0A"
            android:centerY="1.0"
            android:endColor="#743D0A"
            android:angle="270"
            />
    </shape>
    </item>
    <item android:id="@android:id/progress">
    <clip>
        <shape>
            <gradient
                android:startColor="#2A180A"
                android:centerColor="#2A180A"
                android:centerY="1.0"
                android:endColor="#06101d"
                android:angle="270"
                />
        </shape>
    </clip>
    </item>
</layer-list>
